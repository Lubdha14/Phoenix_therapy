{
  "expo": {
    "name": "phoenofix",
    "slug": "phoenofix",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "react-native-vision-camera"
      
    ]
  },
  "android": {
    "permissions": [
      "CAMERA",
      "RECORD_AUDIO"
    ]
  },
  "ios": {
    "permissions": {
      "camera": {
        "description": "Allow the app to use your camera"
      }
    }
  }
}
